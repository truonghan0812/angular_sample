{
  "name": "lesson-on-coding-angular-webpack",
  "version": "1.0.0",
  "description": "Angular webpack starter",
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --inline --hot --progress --port 8080",
    "test": "karma start",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:aot": "ngc -p tsconfig-aot.json",
    "build-aot-prod": "npm run build:aot && npm run webpack-prod",
    "webpack-dev": "npm run clean && cross-env NODE_ENV=development webpack",
    "webpack-prod": "cross-env NODE_ENV=production webpack"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^8.0.0",
    "awesome-typescript-loader": "^3.0.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "imports-loader": "^0.7.1",
    "jasmine-core": "^2.4.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.7.2",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "~2.4.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^3.0.0"
  }
}
